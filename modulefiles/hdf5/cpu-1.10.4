#%Module######################################################################
# hdf5 modulefile
#
proc ModulesHelp { } {
  puts stderr "Sets up the paths you need to use hdf5 version 1.10.4"
}
set sys     [uname sysname]
set version cpu-1.10.4
set tool    hdf5
module-whatis "Hierarchical Data Format Version 5
For further information see https://www.hdfgroup.org/solutions/hdf5/

Compiled with gcc/4.8.5 
"
if { [ module-info mode load ] } {
  if { [is-loaded $tool] && ! [is-loaded $tool/$version] } {
    module unload $tool
  }
}

if {! [module-info mode whatis] } {
setenv       MAALI_HDF5_HOME     /home/app/hdf5/1.10.4/cpu
setenv       HDF5_ROOT           "/home/app/hdf5/1.10.4/cpu"
prepend-path CPATH                /home/app/hdf5/1.10.4/cpu/include
prepend-path CPLUS_INCLUDE_PATH   /home/app/hdf5/1.10.4/cpu/include
prepend-path C_INCLUDE_PATH       /home/app/hdf5/1.10.4/cpu/include
prepend-path LD_LIBRARY_PATH      /home/app/hdf5/1.10.4/cpu/lib
prepend-path PATH                 /home/app/hdf5/1.10.4/cpu/bin
}


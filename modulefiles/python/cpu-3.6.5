#%Module######################################################################
#
# python modulefile
#
proc ModulesHelp { } {
  puts stderr "Sets up the paths you need to use python version 3.6.5"
}

set sys     [uname sysname]
set version cpu-3.6.5
set tool    python
module-whatis "Python is a clear and powerful object-oriented programming language, comparable
to Perl, Ruby, Scheme, or Java.

For further information see https://www.python.org/

Compiled with gcc/4.8.5
"
if { [ module-info mode load ] } {
  if { [is-loaded $tool] && ! [is-loaded $tool/$version] } {
    module unload $tool
  }
}

if {! [module-info mode whatis] } {
setenv       MAALI_PYTHON_HOME          /home/app/python/3.6.5/cpu
setenv       MAALI_PYTHON_LIBDIR        "lib"
setenv       MAALI_PYTHON_LIB_VERSION   "3.6"
prepend-path LD_LIBRARY_PATH            /home/app/python/3.6.5/cpu/lib
prepend-path MANPATH                    /home/app/python/3.6.5/cpu/share/man
prepend-path PATH                       /home/app/python/3.6.5/cpu/bin
prepend-path CPATH                      /home/app/python/3.6.5/cpu/include/python3.6m
prepend-path PKG_CONFIG_PATH            /home/app/python/3.6.5/cpu/lib/pkgconfig
prepend-path PYTHONPATH                 "/home/app/python/3.6.5/cpu/lib/python3.6/site-packages"
setenv       PYTHON_DIR                 "/home/app/python/3.6.5/cpu"
setenv       PYTHON_VERSION             "3.6.5"
}

